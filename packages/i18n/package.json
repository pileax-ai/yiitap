{
	"name": "@yiitap/i18n",
	"description": "Yiitap i18n",
	"version": "0.17.0",
	"private": false,
	"author": {
		"name": "Yiitap",
		"email": "pileaxai@gmail.com"
	},
	"homepage": "",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"src",
		"types"
	],
	"scripts": {
		"build": "pnpm build-lib && pnpm build-types",
		"build-lib": "vite build",
		"build-types": "tsc --emitDeclarationOnly",
		"lint": "eslint 'src/**/*.{ts,vue}'",
		"test": "vitest",
		"test-ci": "vitest --run",
		"generate": "node src/generate/index.js",
		"generate-zh": "node src/generate/index.js --base=zh-Hans"
	},
	"dependencies": {},
	"devDependencies": {
		"@vitalets/google-translate-api": "^9.2.1",
		"bing-translate-api": "^4.2.0",
		"http-proxy-agent": "^7.0.2"
	},
	"peerDependencies": {}
}
